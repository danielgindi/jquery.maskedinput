    }
  ],
  "peerDependencies": {
    "jquery": ">= 1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/runtime": "^7.20.6",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^23.0.4",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/jquery": "^3.5.14",
    "core-js": "^3.26.1",
    "eslint": "^8.29.0",
    "eslint-formatter-codeframe": "^7.32.1",
    "fs-extra": "^11.1.0",
    "husky": "^8.0.2",
    "pinst": "^3.0.0",
    "rollup": "^3.7.0",
    "@rollup/plugin-terser": "^0.2.0"
  }
