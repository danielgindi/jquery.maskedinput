    "@babel/preset-env": "^7.13.15",
    "@babel/runtime": "^7.13.6",
    "core-js": "^3.10.2",
    "eslint": "^7.24.0",
    "fs-extra": "^9.1.0",
    "husky": "^6.0.0",
    "pinst": "^2.1.6",
    "rollup": "^2.45.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
